  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "2.6.0",
    "ember-cli": "2.18.0",
    "ember-cli-dependency-checker": "2.2.1",
    "ember-cli-eslint": "4.2.3",
    "ember-cli-fastboot": "1.1.1",
    "ember-cli-htmlbars-inline-precompile": "2.0.0",
    "ember-cli-inject-live-reload": "1.7.0",
